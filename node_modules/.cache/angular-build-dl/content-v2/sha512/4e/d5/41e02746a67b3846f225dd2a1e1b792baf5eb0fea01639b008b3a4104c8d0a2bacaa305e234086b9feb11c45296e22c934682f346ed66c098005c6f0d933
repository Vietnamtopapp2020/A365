let KienthucchuyenmonPage = class KienthucchuyenmonPage {
    constructor(navCtr, modalController, service, toast, router) {
        this.navCtr = navCtr;
        this.modalController = modalController;
        this.service = service;
        this.toast = toast;
        this.router = router;
        this.list = [];
        this.Getlistitle();
    }
    ngOnInit() {
    }
    Getlistitle() {
        this.service.Getchildren(this.service.getHost() + 'knowledge/topics/topLevel').subscribe(rs => {
            var result = rs.json();
            this.list = result;
        });
    }
    getSafehtml(html) {
        var txt = document.createElement('textarea');
        txt.innerHTML = html;
        return txt.value;
    }
    getdetail(data) {
        let navigationExtras = {
            queryParams: data
        };
        this.router.navigate(['/kienthucchude'], navigationExtras);
    }
    backPage() {
        this.router.navigate(['/dashboard']);
        ;
    }
};
