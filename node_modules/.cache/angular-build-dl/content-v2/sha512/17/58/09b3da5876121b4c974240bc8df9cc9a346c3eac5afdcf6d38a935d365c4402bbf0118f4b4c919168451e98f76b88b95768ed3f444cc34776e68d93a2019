d: \"DC-4\", text: \" Rối loạn phát triển \" },\n    { id: \"DC-2\", text: \" Rối loạn phát triển lan tỏa \" },\n    { id: \"DC-5\", text: \" Chậm phát triển/ Theo dõi chậm phát triển \" },\n    { id: \"DC-8\", text: \" Tăng động, giảm chú ý \" },\n    { id: \"DC-9\", text: \" Rối loạn giao tiếp (chậm nói, ...) \" },\n    { id: \"DC-6\", text: \" Trẻ phát triển bình thường \" },\n    { id: \"DC-7\", text: \" Khác (ghi rõ) \" },\n  ]\n\n  constructor(\n    public navCtr: NavController,\n    public service: ServiceService,\n    public toast: ToastServiceService,\n    public router: Router,\n  ) { }\n  ngOnInit() {\n  }\n  ionViewDidEnter() {\n    this.GetCDR();\n  }\n  GetCDR() {\n    this.service.GetanswersheetsMchartR('https://stage.api.a365.vn/users/me').subscribe\n      (rs => {\n\n        var result = rs.json();\n        this.childrenlist = result.items;\n        this.service.GetanswersheetsMchartR('https://stage.api.a365.vn/children?take=10&').subscribe\n          (rs => {\n\n            var result = rs.json();\n            this.childrenlist = result.items;\n\n          }, error => {\n            this.toast.showToast;\n          });\n\n      })\n\n  }\n  Search() {\n    this.txtname = \"\";\n\n  }\n\n  Searchchildren() {\n    this.service.postGet('https://stage.api.a365.vn/children?skip=' + this.skip + '&take=' + this.take + '&query=' + this.txtname)\n      .subscribe(result => {\n        \n        var rs = result.json();\n        this.childrenlist = rs.items;\n      }, error => {\n        this.toast.showToast;\n      });\n\n  }\n  deletechildren(data) {\n    this.service.postAPIDelete('https://stage.api.a365.vn/children/' + data.id)\n      .subscribe(result => {\n        var rs = result.json();\n        this.service.postGet('https://stage.api.a365.vn/children?skip=' + this.skip + '&take=' + this.take + '&query=' + this.query).subscribe\n          (rs => {\n            var result = rs.json();\n            this.childrenlist = result.items;\n          }, error => {\n            this.toast.showToast;\n          });\n      }, error => {\n        this.toast.showToast;\n      });\n\n  }\n\n  ASQ(data) {\n    let navigationExtras: NavigationExtras = {\n      queryParams: data\n    };\n    this.router.navigate(['/asqtest'], navigationExtras);\n  }\n\n  ATEC(data) {\n    let navigationExtras: NavigationExtras = {\n      queryParams: data\n    };\n    this.router.navigate(['/atecview'], navigationExtras);\n  }\n\n  Update(data) {\n    \n    let navigationExtras: NavigationExtras = {\n      queryParams: data\n    };\n    this.router.navigate(['/updatechildren'], navigationExtras);\n\n\n  }\n  TestATEC(data) {\n    \n    var checkTestATEC = false;\n    for (var i = 0; i < this.DC.length; i++) {\n      if (data.diagnosticConclusion == this.DC[i].id) {\n        checkTestATEC = true;\n        break;\n      }\n    }\n    if (checkTestATEC == true) {\n      this.ATEC(data);\n    }\n    else {\n      this.service.message(\"Trẻ không đủ điều kiện để làm bài test\")\n    }\n\n  }\n\n  Addchilren() {\n    this.router.navigate(['/createchildren']);\n  }\n  backPage() {\n    this.router.navigate(['/dashboard']);\n  }\n\n  ViewSearch() {\n    if (this.searchCheck) {\n      this.searchCheck = false;\n      this.txtname = \"\";\n    }\n    else {\n      this.searchCheck = true;\n      this.txtname = \"txtname\";\n    }\n  }\n\n\n}\n"]}