plateUrl: './kienthucchude.page.html',\n  styleUrls: ['./kienthucchude.page.scss'],\n})\nexport class KienthucchudePage implements OnInit {\n  data = null;\n  listchude = [];\n  listchude2 = [];\n  listchude3 = [];\n  listPostschildren = [];\n  constructor(\n    private service: ServiceService,\n    private router: Router,\n    public navCtr: NavController,\n\n  ) {\n    \n    this.data = this.router.getCurrentNavigation().extras.queryParams;\n\n  }\n  ngOnInit() {\n    this.Getdetail();\n  }\n  Getdetail() {\n\n    this.service.Getchildren(this.service.getHost() + 'knowledge/topics/' + this.data.slug).subscribe(rs => {\n      var result = rs.json();\n      \n      this.listchude = result.children;\n      this.listchude.forEach(element => {\n        this.service.Getchildren(this.service.getHost() + 'knowledge/posts/search?topic=' + element.id + '&take=20&').subscribe(rs => {\n          var result = rs.json();\n          this.listPostschildren.push({\n            children: element.children,\n            createdAt: element.createdAt,\n            description: element.description,\n            hexCode: element.hexCode,\n            id: element.id,\n            key: element.key,\n            order: element.order,\n            parent: element.parent,\n            slug: element.slug,\n            title: element.title,\n            updatedAt: element.updatedAt,\n            listPosts: result.items,\n            checkViewPost: false\n          });\n        });\n        \n      });\n    });\n  }\n  backPage() {\n    this.navCtr.pop();\n  }\n  public getSafehtml(html: string) {\n    var txt = document.createElement('textarea');\n    txt.innerHTML = html\n    return txt.value;\n  }\n  ViewListPost(Postschildren) {\n    Postschildren.checkViewPost = !Postschildren.checkViewPost;\n  }\n  getdetailPost(data) {\n    let navigationExtras: NavigationExtras = {\n      queryParams: data\n    };\n    this.router.navigate(['/postdetail'], navigationExtras);\n  }\n}\n"]}