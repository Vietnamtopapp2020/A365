"use strict";
__webpack_require__.r(__webpack_exports__);
/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "KienthucchudePage", function() { return KienthucchudePage; });
/* harmony import */ var tslib__WEBPACK_IMPORTED_MODULE_0__ = __webpack_require__(/*! tslib */ "./node_modules/tslib/tslib.es6.js");
/* harmony import */ var _angular_core__WEBPACK_IMPORTED_MODULE_1__ = __webpack_require__(/*! @angular/core */ "./node_modules/@angular/core/__ivy_ngcc__/fesm2015/core.js");
/* harmony import */ var _angular_router__WEBPACK_IMPORTED_MODULE_2__ = __webpack_require__(/*! @angular/router */ "./node_modules/@angular/router/__ivy_ngcc__/fesm2015/router.js");
/* harmony import */ var src_app_providers_service_service__WEBPACK_IMPORTED_MODULE_3__ = __webpack_require__(/*! src/app/providers/service.service */ "./src/app/providers/service.service.ts");
/* harmony import */ var _ionic_angular__WEBPACK_IMPORTED_MODULE_4__ = __webpack_require__(/*! @ionic/angular */ "./node_modules/@ionic/angular/__ivy_ngcc__/fesm2015/ionic-angular.js");





let KienthucchudePage = class KienthucchudePage {
    constructor(service, router, navCtr) {
        this.service = service;
        this.router = router;
        this.navCtr = navCtr;
        this.data = null;
        this.listchude = [];
        this.listchude2 = [];
        this.listchude3 = [];
        this.listPostschildren = [];
        this.title = "";
        this.data = this.router.getCurrentNavigation().extras.queryParams;
        this.title = this.data.title;
        this.Getdetail();
    }
    ngOnInit() {
    }
    Getdetail() {
        this.service.Getchildren(this.service.getHost() + 'knowledge/topics/' + this.data.slug).subscribe(rs => {
            var result = rs.json();
            this.listchude = result.children;
            this.listchude.forEach(element => {
                this.service.Getchildren(this.service.getHost() + 'knowledge/posts/search?topic=' + element.id + '&take=20&').subscribe(rs => {
                    var result = rs.json();
                    this.listPostschildren.push({
                        children: element.children,
                        createdAt: element.createdAt,
                        description: element.description,
                        hexCode: element.hexCode,
                        id: element.id,
                        key: element.key,
                        order: element.order,
                        parent: element.parent,
                        slug: element.slug,
                        title: element.title,
                        updatedAt: element.updatedAt,
                        listPosts: result.items,
                        checkViewPost: false
                    });
                });
            });
        });
    }
    backPage() {
        this.navCtr.pop();
    }
    getSafehtml(html) {
        var txt = document.createElement('textarea');
        txt.innerHTML = html;
        return txt.value;
    }
    ViewListPost(Postschildren) {
        Postschildren.checkViewPost = !Postschildren.checkViewPost;
    }
    getdetailPost(data) {
        let navigationExtras = {
            queryParams: data
        };
        this.router.navigate(['/postdetail'], navigationExtras);
    }
};
KienthucchudePage.ctorParameters = () => [
    { type: src_app_providers_service_service__WEBPACK_IMPORTED_MODULE_3__["ServiceService"] },
    { type: _angular_router__WEBPACK_IMPORTED_MODULE_2__["Router"] },
    { type: _ionic_angular__WEBPACK_IMPORTED_MODULE_4__["NavController"] }
];
KienthucchudePage = Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__decorate"])([
    Object(_angular_core__WEBPACK_IMPORTED_MODULE_1__["Component"])({
        selector: 'app-kienthucchude',
        template: Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__importDefault"])(__webpack_require__(/*! raw-loader!./kienthucchude.page.html */ "./node_modules/raw-loader/dist/cjs.js!./src/app/page/kienthucchude/kienthucchude.page.html")).default,
        styles: [Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__importDefault"])(__webpack_require__(/*! ./kienthucchude.page.scss */ "./src/app/page/kienthucchude/kienthucchude.page.scss")).default]
    })
], KienthucchudePage);



