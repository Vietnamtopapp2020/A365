"use strict";
__webpack_require__.r(__webpack_exports__);
/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "KetquaasqPage", function() { return KetquaasqPage; });
/* harmony import */ var tslib__WEBPACK_IMPORTED_MODULE_0__ = __webpack_require__(/*! tslib */ "./node_modules/tslib/tslib.es6.js");
/* harmony import */ var _angular_core__WEBPACK_IMPORTED_MODULE_1__ = __webpack_require__(/*! @angular/core */ "./node_modules/@angular/core/__ivy_ngcc__/fesm2015/core.js");
/* harmony import */ var src_app_providers_service_service__WEBPACK_IMPORTED_MODULE_2__ = __webpack_require__(/*! src/app/providers/service.service */ "./src/app/providers/service.service.ts");
/* harmony import */ var src_app_providers_toast_service_service__WEBPACK_IMPORTED_MODULE_3__ = __webpack_require__(/*! src/app/providers/toast-service.service */ "./src/app/providers/toast-service.service.ts");
/* harmony import */ var _angular_router__WEBPACK_IMPORTED_MODULE_4__ = __webpack_require__(/*! @angular/router */ "./node_modules/@angular/router/__ivy_ngcc__/fesm2015/router.js");
/* harmony import */ var _ionic_angular__WEBPACK_IMPORTED_MODULE_5__ = __webpack_require__(/*! @ionic/angular */ "./node_modules/@ionic/angular/__ivy_ngcc__/fesm2015/ionic-angular.js");
/* harmony import */ var moment__WEBPACK_IMPORTED_MODULE_6__ = __webpack_require__(/*! moment */ "./node_modules/moment/moment.js");
/* harmony import */ var moment__WEBPACK_IMPORTED_MODULE_6___default = /*#__PURE__*/__webpack_require__.n(moment__WEBPACK_IMPORTED_MODULE_6__);







let KetquaasqPage = class KetquaasqPage {
    constructor(navCtr, service, router, toast) {
        this.navCtr = navCtr;
        this.service = service;
        this.router = router;
        this.toast = toast;
        this.children = null;
        this.communicationAnswers = [];
        this.grossMotorAnswers = [];
        this.fineMotorAnswers = [];
        this.problemSolvingAnswers = [];
        this.personalSocialAnswers = [];
        this.overallAnswers = [];
        this.notifi = [{
                text: 'Lưu thành công'
            }];
        this.firstActionOption = 0;
        this.CCommunication = 0;
        this.CFineMotor = 0;
        this.CGrossMotor = 0;
        this.CPersonalSocial = 0;
        this.CProblemSolving = 0;
        this.MCommunication = 0;
        this.MFineMotor = 0;
        this.MGrossMotor = 0;
        this.MPersonalSocial = 0;
        this.MProblemSolving = 0;
        this.SCommunication = 0;
        this.SFineMotor = 0;
        this.SGrossMotor = 0;
        this.SPersonalSocial = 0;
        this.SProblemSolving = 0;
        this.TitletHard = [];
        this.TitleFollow = [];
        this.TitletNormal = [];
        this.secondActions = [];
        this.nextQuestionnareSchedule = {
            months: 0,
            days: 0,
        };
        this.data = this.router.getCurrentNavigation().extras.queryParams;
        this.GetResutlASQ();
    }
    ngOnInit() {
    }
    GetResutlASQ() {
        this.service.postGet(this.service.getHost() + 'asq/answersheets/' + this.data).subscribe(rs => {
            var result = rs.json();
            this.children = result;
            this.name = this.children.child.name;
            this.cid = this.children.child.id;
            this.childid = this.children.child.id.substr(length - 5);
            this.dob = this.children.child.birthDate.day + "-" + this.children.child.birthDate.month + "-" + this.children.child.birthDate.year;
            this.nameanwser = this.children.questionnare.name;
            this.createat = moment__WEBPACK_IMPORTED_MODULE_6__(this.children.createdAt).format("DD-MM-YYYY");
            this.score = this.children.score;
            this.postname = this.children.questionnare.name;
            this.isEligibleForMchatr = this.children.overallAnswersSuggestMCHAT;
            // phan 3
            this.SCommunication = this.children.scoreByField.communication / 60 * 80 + 2;
            this.SFineMotor = this.children.scoreByField.fineMotor / 60 * 80 + 2;
            this.SGrossMotor = this.children.scoreByField.grossMotor / 60 * 80 + 2;
            this.SPersonalSocial = this.children.scoreByField.personalSocial / 60 * 80 + 2;
            this.SProblemSolving = this.children.scoreByField.problemSolving / 60 * 80 + 2;
            this.communicationscore = this.children.scoreByField.communication;
            this.fineMotorscore = this.children.scoreByField.fineMotor;
            this.grossMotorscore = this.children.scoreByField.grossMotor;
            this.personalSocialscore = this.children.scoreByField.personalSocial;
            this.problemSolvingscore = this.children.scoreByField.problemSolving;
            this.communicationThreshold = this.children.questionnare.cutoffPoint.communication;
            this.fineMotorThreshold = this.children.questionnare.cutoffPoint.fineMotor;
            this.grossMotorThreshold = this.children.questionnare.cutoffPoint.grossMotor;
            this.personalSocialThreshold = this.children.questionnare.cutoffPoint.personalSocial;
            this.problemSolvingThreshold = this.children.questionnare.cutoffPoint.problemSolving;
            this.MCommunication = this.children.questionnare.monitoringPoint.communication / 60 * 80;
            this.MFineMotor = this.children.questionnare.monitoringPoint.fineMotor / 60 * 80;
            this.MGrossMotor = this.children.questionnare.monitoringPoint.grossMotor / 60 * 80;
            this.MPersonalSocial = this.children.questionnare.monitoringPoint.personalSocial / 60 * 80;
            this.MProblemSolving = this.children.questionnare.monitoringPoint.problemSolving / 60 * 80;
            this.CCommunication = this.children.questionnare.cutoffPoint.communication / 60 * 80;
            this.CFineMotor = this.children.questionnare.cutoffPoint.fineMotor / 60 * 80;
            this.CGrossMotor = this.children.questionnare.cutoffPoint.grossMotor / 60 * 80;
            this.CPersonalSocial = this.children.questionnare.cutoffPoint.personalSocial / 60 * 80;
            this.CProblemSolving = this.children.questionnare.cutoffPoint.problemSolving / 60 * 80;
            // phàn 4
            if (this.children.scoreByField.communication <= this.children.questionnare.cutoffPoint.communication) {
                this.TitletHard.push("Giao tiếp");
            }
            else if (this.children.scoreByField.communication <= this.children.questionnare.monitoringPoint.communication) {
                this.TitleFollow.push("Giao tiếp");
            }
            else {
                this.TitletNormal.push("Giao tiếp");
            }
            if (this.children.scoreByField.grossMotor <= this.children.questionnare.cutoffPoint.grossMotor) {
                this.TitletHard.push('Vận động thô');
            }
            else if (this.children.scoreByField.grossMotor <= this.children.questionnare.monitoringPoint.grossMotor) {
                this.TitleFollow.push('Vận động thô');
            }
            else {
                this.TitletNormal.push('Vận động thô');
            }
            if (this.children.scoreByField.fineMotor <= this.children.questionnare.cutoffPoint.fineMotor) {
                this.TitletHard.push("Vận động tĩnh");
            }
            else if (this.children.scoreByField.fineMotor <= this.children.questionnare.monitoringPoint.fineMotor) {
                this.TitleFollow.push("Vận động tĩnh");
            }
            else {
                this.TitletNormal.push("Vận động tĩnh");
            }
            if (this.children.scoreByField.problemSolving <= this.children.questionnare.cutoffPoint.problemSolving) {
                this.TitletHard.push("Giải quyết vấn đề");
            }
            else if (this.children.scoreByField.problemSolving <= this.children.questionnare.monitoringPoint.problemSolving) {
                this.TitleFollow.push("Giải quyết vấn đề");
            }
            else {
                this.TitletNormal.push("Giải quyết vấn đề");
            }
            if (this.children.scoreByField.personalSocial <= this.children.questionnare.cutoffPoint.personalSocial) {
                this.TitletHard.push("Cá nhân xã hội");
            }
            else if (this.children.scoreByField.personalSocial <= this.children.questionnare.monitoringPoint.personalSocial) {
                this.TitleFollow.push("Cá nhân xã hội");
            }
            else {
                this.TitletNormal.push("Cá nhân xã hội");
            }
            // phần 5
            debugger;
            if (this.TitletHard.length == 0 && this.TitleFollow.length > 0 && this.TitleFollow.length <= 3 && this.isEligibleForMchatr) {
                var check = false;
                for (var i = 0; this.TitleFollow.length; i++) {
                    if (this.TitleFollow[i] != "Giao tiếp" || this.TitleFollow[i] != "Giải quyết vấn đề" || this.TitleFollow[i] != "Cá nhân xã hội") {
                        check = true;
                        break;
                    }
                }
                if (check) {
                    if (this.children.questionnare.upperMonths >= 16 && this.children.questionnare.upperMonths <= 48) {
                        this.isEligibleForMchatr = true;
                    }
                    else {
                        this.isEligibleForMchatr = false;
                    }
                    this.firstActionOption = 5;
                }
            }
            else if (this.TitletNormal.length == 5 && this.isEligibleForMchatr) {
                if (this.children.questionnare.upperMonths >= 16 && this.children.questionnare.upperMonths <= 48) {
                    this.isEligibleForMchatr = true;
                }
                else {
                    this.isEligibleForMchatr = false;
                }
                this.firstActionOption = 4;
                this.getNextQuestionnareSchedule();
            }
            else if (this.TitletHard.length > 0 && this.TitleFollow.length > 0) {
                var check = false;
                for (var i = 0; this.TitleFollow.length; i++) {
                    if (this.TitleFollow[i] == "Giao tiếp" || this.TitleFollow[i] == "Giải quyết vấn đề" || this.TitleFollow[i] == "Cá nhân xã hội") {
                        check = true;
                        break;
                    }
                }
                if (check) {
                    this.firstActionOption = 3;
                }
            }
            else if (this.TitletHard.length == 0 && this.TitleFollow.length > 0 && this.TitleFollow.length <= 3) {
                var check = false;
                for (var i = 0; this.TitleFollow.length; i++) {
                    if (this.TitleFollow[i] != "Giao tiếp" && this.TitleFollow[i] != "Giải quyết vấn đề" && this.TitleFollow[i] != "Cá nhân xã hội") {
                        check = true;
                        break;
                    }
                }
                if (check) {
                    this.firstActionOption = 2;
                }
            }
            else {
                // else (this.TitletNormal.length == 5 && !this.isEligibleForMchatr) {
                this.firstActionOption = 1;
                this.getNextQuestionnareSchedule();
            }
            for (var i = 0; i < this.children.overallAnswers.length; i++) {
                if (this.children.overallAnswers[i].score == 0) {
                    if (this.secondActions.length == 0) {
                        this.secondActions.push(this.children.questionnare.overallQuestions[i].referDoctorText);
                    }
                    else {
                        var check = true;
                        this.secondActions.forEach(element => {
                            if (element == this.children.questionnare.overallQuestions[i].referDoctorText) {
                                check = false;
                            }
                        });
                        if (check) {
                            this.secondActions.push(this.children.questionnare.overallQuestions[i].referDoctorText);
                        }
                    }
                }
            }
            //
        }, error => {
            this.toast.showToast;
        });
    }
    getNextQuestionnareSchedule() {
        this.service.postGet("https://stage.api.a365.vn/asq/questionnares/getNextQuestionnareSchedule?childId=5efab51ad9db8918be3997ba").subscribe(rs => {
            var result = rs.json();
            this.nextQuestionnareSchedule = result;
        });
    }
    MchartR() {
        let navigationExtras = {
            queryParams: this.children.child
        };
        this.router.navigate(['/mchar-r'], navigationExtras);
    }
};
