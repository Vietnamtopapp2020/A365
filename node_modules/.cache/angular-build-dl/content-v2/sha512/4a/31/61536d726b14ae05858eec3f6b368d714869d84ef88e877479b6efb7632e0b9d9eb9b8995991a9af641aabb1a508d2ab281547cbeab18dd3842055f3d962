rl.create({\n      component: UpdateprofilePage,\n      componentProps: {\n        txtEmail: this.txtEmail,\n        id: id\n      },\n      cssClass: \"modalmesssc\"\n\n    });\n    myModal.onDidDismiss().then(rs => {\n    });\n    return await myModal.present();\n  }\n\n  async moveTofirstStaff(id) {\n    const myModal = await this.modalCtrl.create({\n      component: UpdateprofilestaffPage,\n      componentProps: {\n        txtEmail: this.txtEmail,\n        id: id\n      },\n      cssClass: \"modalmessscStaff\"\n    });\n    myModal.onDidDismiss().then(rs => {\n\n    });\n    return await myModal.present();\n  }\n\n  registerUser() {\n    var body = {\n      \"email\": this.txtEmail,\n      \"password\": this.txtPassword,\n      \"register\": this.txtPasswordregister,\n      \"role\": parseInt(this.txtRole),\n    };\n    this.service.postAPIregister('https://stage.api.a365.vn/auth/register', body).subscribe\n      (rs => {\n        var result = rs.json();\n        this.service.tokenId = result.accessToken;\n        this.service.postPushNotification('https://stage.api.a365.vn/users/me').subscribe(\n          rs1 => {\n            var result1 = rs1.json();\n            if (parseInt(this.txtRole) == 1 || parseInt(this.txtRole) == 2) {\n\n              this.moveToFirst(result1._id);\n\n            }\n            if (parseInt(this.txtRole) == 3) {\n\n              this.moveTofirstStaff(result1._id);\n            }\n          }\n        )\n\n      }), (error) => {\n      }\n  }\n  backPage() {\n    this.navCtrl.navigateRoot('/login');\n  }\n}\n"]}